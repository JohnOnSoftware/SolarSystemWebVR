    "ovrui": "^1.1.0",
    "react": "~15.4.1",
    "react-native": "~0.42.0",
    "react-vr": "^1.1.0",
    "react-vr-web": "^1.1.0",
    "three": "^0.80.1"
  },
  "devDependencies": {
    "babel-jest": "^19.0.0",
    "babel-preset-react-native": "^1.9.1",
    "jest": "^19.0.2",
    "react-devtools": "^2.1.3",
    "react-test-renderer": "~15.4.1",
    "xopen": "1.0.0"
  },
  "jest": {
    "preset": "react-vr"
  }
}
